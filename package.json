  "scripts": {
    "build": "nest build",
    "start": "dotenv -e .env.prod -- nest start",
    "start:dev": "dotenv -e .env.dev -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "test": "dotenv -e .env.test -- jest",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:cov": "dotenv -e .env.test -- jest --coverage",
    "prisma:format": "npx prisma format",
    "prisma:generate": "prisma generate",
    "prisma:push:dev": "dotenv -e .env.dev -- npx prisma db push && npm run prisma:generate",
    "prisma:migrate:dev": "dotenv -e .env.dev -- prisma migrate dev",
    "prisma:reset:dev": "dotenv -e .env.dev -- npx prisma migrate reset && npm run prisma:generate",
    "prisma:push:test": "dotenv -e .env.test -- npx prisma db push && npm run prisma:generate",
    "prisma:migrate:test": "dotenv -e .env.test -- prisma migrate dev",
    "prisma:reset:test": "dotenv -e .env.test -- npx prisma migrate reset && npm run prisma:generate"
  },
